defaultEntryPoints = ["http", "https"]
logLevel = "INFO"

[entryPoints]
  [entryPoints.http]
    address = ":80"
  [entryPoints.https]
    address = ":443"
    [entryPoints.https.tls]

[acme]
email = "info@gitea.io"
storageFile = "/etc/acme.json"
entryPoint = "https"
onDemand = true

[docker]
domain = "gitea.io"
watch = true
